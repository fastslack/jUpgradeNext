{
    "name": "matware-lab/jupgradenext",
    "type": "joomla-package",
    "description": "jUpgradeNext is a Joomla! Framework extension that allow you to move/migrate your data from one Joomla! installation to another one.",
    "keywords": ["joomla", "migration", "framework", "upgrade"],
    "homepage": "https://github.com/matware-lab/jUpgradeNext",
    "license": "GPL-2.0+",
    "version": "1.0",
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require": {
        "php": "^5.3.10|~7.0",
        "joomla/oauth2": "^1.1.4",
        "joomla/input": "~1.0",
        "joomla/http": "~1.0",
        "joomla/uri": "~1.0",
				"joomla/di": "~1.3",
				"joomla/database": "~1.0",
        "monolog/monolog": "1.*@stable",
        "joomla/event": "2.0.*@dev",
        "joomla/language": "~1.0",
        "joomla/model": "~1.0",
        "joomla/filesystem": "~1.0",
        "joomla/date": "2.0.*@dev",
        "joomla/string": "~1.0",
        "matware-lab/access": "~1.0",
        "matware-lab/table": "~1.0",
        "ghostff/dump5": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Providers\\": "src/Providers",
            "JupgradeNext\\": "src/",
            "Jupgradenext\\Drivers\\": "src/Drivers",
            "Jupgradenext\\Models\\": "src/Models",
            "Jupgradenext\\Schemas\\v10\\": "src/Schemas/v10",
            "Jupgradenext\\Schemas\\v15\\": "src/Schemas/v15",
            "Jupgradenext\\Schemas\\v16\\": "src/Schemas/v16",
            "Jupgradenext\\Schemas\\v17\\": "src/Schemas/v17",
            "Jupgradenext\\Schemas\\v25\\": "src/Schemas/v25",
            "Jupgradenext\\Schemas\\v30\\": "src/Schemas/v30",
            "Jupgradenext\\Schemas\\v31\\": "src/Schemas/v31",
            "Jupgradenext\\Schemas\\v32\\": "src/Schemas/v32",
            "Jupgradenext\\Schemas\\v33\\": "src/Schemas/v33",
            "Jupgradenext\\Schemas\\v34\\": "src/Schemas/v34",
            "Jupgradenext\\Schemas\\v35\\": "src/Schemas/v35",
            "Jupgradenext\\Schemas\\v36\\": "src/Schemas/v36",
            "Jupgradenext\\Schemas\\v40\\": "src/Schemas/v40",
            "Jupgradenext\\Steps\\": "src/Steps",
            "Jupgradenext\\Upgrade\\": "src/Upgrade"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jUpgradeNext\\Tests\\": "Tests/"
        }
    }
}
